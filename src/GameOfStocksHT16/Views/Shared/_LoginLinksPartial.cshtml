@using Microsoft.AspNetCore.Identity
@using GameOfStocksHT16.Models

@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@if (!SignInManager.IsSignedIn(User))
{

    <a asp-area="" asp-controller="Account" asp-action="Login" class="btn btn-primary">Logga in</a>
        <a asp-area="" asp-controller="Account" asp-action="Register" class="btn btn-success">Registrera</a>
}
